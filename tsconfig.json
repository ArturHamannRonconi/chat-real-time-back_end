{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@appTypes/*": ["@types/appTypes/*"],
      "@config/*": ["config/*"],
      "@accounts/*": ["modules/accounts/*"],
      "@chats/*": ["modules/chats/*"],
      "@utils/*": ["utils/*"],
      "@shared/*": ["shared/*"],
      
    },
    "forceConsistentCasingInFileNames": true
  }
}
